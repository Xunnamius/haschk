{
    "source": "./src",
    "destination": "../docs/documentation/",
    "plugins": [
        {
            "name": "esdoc-node"
        },
        {
            "name": "esdoc-standard-plugin"
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "classProperties": true,
                "objectRestSpread": true,
                "doExpressions": true,
                "functionBind": true,
                "functionSent": true,
                "asyncGenerators": true,
                "decorators": true,
                "exportExtensions": true,
                "dynamicImport": true
            }
        }]
}
