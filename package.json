{
  "name": "haschk",
  "description": "A proof-of-concept web extension tailored to Google Chrome that implements the HASCHK protocol",
  "repository": {
    "type": "git",
    "url": "https://git.xunn.io/closed-source/research/psd-mirrored/haschk"
  },
  "version": "0.11.0",
  "scripts": {
    "dev": "npx gulp wpdevserv",
    "build": "npx gulp build",
    "bundle": "npx gulp bundleZip",
    "test": "npx jest",
    "test-ib": "npx jest --runInBand",
    "test-nc": "npx jest --no-cache",
    "install-types": "npx flow-typed install",
    "debug-remote": "node --inspect-brk='0.0.0.0:7777'",
    "debug-remote-gulp": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/gulp",
    "debug-remote-jest": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/jest --runInBand",
    "clean-build": "npx gulp cleanBuild",
    "clean-types": "npx gulp cleanTypes",
    "generate": "BABEL_ENV=generator npx babel config --out-dir .",
    "regenerate": "npx gulp regenerate",
    "bump-major": "npx bump --major --tag --commit --lock --all",
    "bump-minor": "npx bump --minor --tag --commit --lock --all",
    "bump-patch": "npx bump --patch --tag --commit --lock --all",
    "list-tasks": "npm run && echo 'Compiling gulp task list...' && npx gulp --tasks",
    "preversion": "npm run test-ib",
    "version": "git add --all"
  },
  "author": "Xunnamius",
  "license": "MIT",
  "main": "./src/index.js",
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^universe$": "<rootDir>/src/universe",
      "^universe/(.*)$": "<rootDir>/src/universe/$1",
      "^components/(.*)$": "<rootDir>/src/components/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "globals": {
      "_NODE_ENV": "test",
      "_HASHING_ALGORITHM": "",
      "_APPLICATION_LABEL": "",
      "_MAX_REQUEST_HISTORY": "",
      "_BASE32_URN_LENGTH": "",
      "URN_PREFIX": ""
    }
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "array-buffer-to-hex": "^1.0.0",
    "axios": "^0.19.2",
    "base32-encode": "^1.1.1",
    "dotenv": "^8.2.0",
    "eventemitter3": "^4.0.0",
    "hex-to-array-buffer": "^1.1.0",
    "install": "^0.13.0",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/node": "^7.8.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-source-map-support": "^2.1.1",
    "bluebird": "^3.7.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "fancy-log": "^1.3.3",
    "file-loader": "^5.0.2",
    "flow-bin": "^0.118.0",
    "flow-typed": "^2.6.2",
    "gulp": "^4.0.2",
    "gulp-tap": "^2.0.0",
    "gulp-zip": "^5.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jsdoc": "^3.6.3",
    "jsdoc-babel": "^0.5.0",
    "parse-gitignore": "^1.0.1",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.16",
    "stream-array": "^1.1.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "version-bump-prompt": "^6.0.0",
    "vinyl-fs": "^3.0.3",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-extension-reloader": "^1.1.4",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
