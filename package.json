{
  "name": "dnschk-chrome-webextension",
  "description": "A pre-alpha proof-of-concept WebExtension tailored to Google Chrome that implements the DNSCHK protocol.",
  "repository": {
    "type": "git",
    "url": "https://github.com/morty-c137-prime/DNSCHK"
  },
  "version": "0.1.0",
  "scripts": {
    "test": "npx jest",
    "test-nc": "npx jest --no-cache",
    "install-types": "npx flow-typed install",
    "clean-build": "npx gulp cleanBuild",
    "clean-types": "npx gulp cleanTypes",
    "regenerate": "npx gulp regenerate",
    "generate": "BABEL_ENV=generator npx babel config --out-dir .",
    "debug-remote": "node --inspect-brk='0.0.0.0:7777'",
    "debug-remote-gulp": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/gulp",
    "debug-remote-jest": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/jest --runInBand",
    "doc": "npx jsdoc -c config/.jsdoc.json -d docs/documentation/ -r",
    "build": "npx gulp build",
    "dev": "npx gulp wpdevserv"
  },
  "author": "Richard Alvarez",
  "license": "MIT",
  "main": "./src/index.js",
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.preprocess.js"
    },
    "moduleNameMapper": {
      "^universe$": "<rootDir>/src/universe",
      "^universe/(.*)$": "<rootDir>/src/universe/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/components/background/"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "eventemitter3": "^3.1.0",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "del": "^3.0.0",
    "documentation": "^8.1.2",
    "documentation-devseed-theme": "^0.1.4",
    "eslint": "^5.8.0",
    "eslint-plugin-flowtype": "^3.1.4",
    "fancy-log": "^1.3.2",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.85.0",
    "flow-typed": "^2.5.1",
    "gulp": "^4.0.0",
    "gulp-tap": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.5.0",
    "parse-gitignore": "^1.0.1",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.9",
    "stream-array": "^1.1.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0",
    "vinyl-fs": "^3.0.3",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-server": "^3.1.10",
    "write-file-webpack-plugin": "^4.4.1"
  }
}
