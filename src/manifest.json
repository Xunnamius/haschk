{
    "manifest_version": 2,
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "file://*",
        "downloads",
        "<all_urls>"
    ],
    "icons": {
        "16": "assets/icon/nuetral/16x16.png",
        "57": "assets/icon/nuetral/57x57.png",
        "120": "assets/icon/nuetral/120x120.png",
        "310": "assets/icon/nuetral/310x310.png"
    }
}
