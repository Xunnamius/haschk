{
    "manifest_version": 2,
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/icon/16x16.png",
            "57": "assets/icon/57x57.png",
            "120": "assets/icon/120x120.png",
            "310": "assets/icon/310x310.png"
        }
    },
    "options_page": "options.html",
    "permissions": [
        "file://*",
        "<all_urls>",
        "downloads",
        "tabs",
        "storage"
    ],
    "icons": {
        "16": "assets/icon/neutral/16x16.png",
        "57": "assets/icon/neutral/57x57.png",
        "120": "assets/icon/neutral/120x120.png",
        "310": "assets/icon/neutral/310x310.png"
    }
}
